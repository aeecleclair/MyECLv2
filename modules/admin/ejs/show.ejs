<div class="content">
    <div class="main-content">
        Nom : <input id="input_name" type="text" name="name" value="<%=name%>"><br/>
        Description : <textarea id="input_desc"><%=description%></textarea><br/>
        <button id="save_fields">Enregistrer les champs</button></br>
        Membres :
        <% for(let i in members){ %>
            <div>
                <%=members[i].name%> <%=members[i].firstname%> (<%=members[i].nick%>), <%=members[i].position%><br/>
            </div>
        <% } %>
        <button>Ajouter un membre</button>
        <input hidden id="hidden_id" value="<%=id%>">
    </div>
</div>
